<view><cu-custom vue-id="1" bgColor="bg-gradual-orange" isBack="{{true}}" onVueInit="__l"><view slot="backText"></view><view slot="content">编辑</view></cu-custom><scroll-view class="page bg-white " scroll-y><view class="cu-bar bg-white justify-end"><view class="content">请输入输赢信息</view></view><view class="cu-form-group margin-top"><view class="title">娱乐类型</view><picker value="{{LstType}}" range="{{picker}}" data-event-opts="{{[['change',[['PickerChange',['$event']]]]]}}" onChange="__e"><view class="picker">{{LstType>-1?picker[LstType]:picker[0]}}</view></picker></view><view class="cu-form-group"><view class="title">娱乐场所</view><input style="text-align:left;" type="text" name="address" data-event-opts="{{[['input',[['inputAddress',['$event']]]]]}}" value="{{LstAddress}}" onInput="__e"/></view><view class="cu-form-group"><view class="title">日期选择</view><picker mode="date" value="{{LstDate}}" start="2018-09-01" end="2050-09-01" data-event-opts="{{[['change',[['DateChange',['$event']]]]]}}" onChange="__e"><view class="picker">{{LstDate}}</view></picker></view><view class="cu-form-group flex"><view class="title">输赢类别</view><view data-event-opts="{{[['tap',[['ChangeType',[1]]]]]}}" class="{{(('flex-sub  cu-tag ')+' '+(LstWIN==2?'':'cu-tag bg-orange'))}}" onTap="__e">略有所得</view><view data-event-opts="{{[['tap',[['ChangeType',[2]]]]]}}" class="{{(('flex-sub  cu-tag ')+' '+(LstWIN==1?'':'cu-tag bg-orange'))}}" onTap="__e">少输当赢</view></view><view class="cu-form-group"><view class="title">涉及金额</view><input style="text-align:right;" type="number" name="Money" data-event-opts="{{[['input',[['inputMoney',['$event']]]]]}}" value="{{LstMoney}}" onInput="__e"/></view><view class="cu-form-group"><view class="title">上手姓名</view><input style="text-align:left;" type="text" name="Friend1" data-event-opts="{{[['input',[['inputFriend1',['$event']]]]]}}" value="{{friend1}}" onInput="__e"/></view><view class="cu-form-group"><view class="title">对家姓名</view><input style="text-align:left;" type="text" name="Friend2" data-event-opts="{{[['input',[['inputFriend2',['$event']]]]]}}" value="{{friend2}}" onInput="__e"/></view><view class="cu-form-group"><view class="title">下手姓名</view><input style="text-align:left;" type="text" name="Friend3" data-event-opts="{{[['input',[['inputFriend3',['$event']]]]]}}" value="{{friend3}}" onInput="__e"/></view><view class="cu-form-group"><view class="title">围观人员</view><input style="text-align:left;" type="text" name="Friend4" data-event-opts="{{[['input',[['inputFriend4',['$event']]]]]}}" value="{{friend4}}" onInput="__e"/></view><view class="cu-form-group justify-end"><view class="action "><button data-event-opts="{{[['tap',[['go_back',['$event']]]]]}}" class="cu-btn line-orange text-orange" onTap="__e">取消</button><button data-event-opts="{{[['tap',[['go_save_list',['$event']]]]]}}" class="cu-btn bg-orange margin-left" onTap="__e">保存</button></view></view></scroll-view></view>