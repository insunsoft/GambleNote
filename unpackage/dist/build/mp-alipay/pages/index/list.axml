<view><cu-custom vue-id="1" bgColor="bg-gradual-pink" isBack="{{true}}" onVueInit="__l"><view slot="backText">返回</view><view slot="content">输赢记录</view></cu-custom><block a:for="{{WinList}}" a:for-item="item" a:for-index="index" a:key="index"><view class="cu-list menu-avatar comment solids-top"><view class="cu-item"><view class="{{(('cu-avatar radius ')+' '+(item.LstWIN==1?'bg-orange':'bg-cyan'))}}"><text>{{item.LstWIN==1?'赢':'输'}}</text></view><view class="content"><view></view><view class="{{(('cu-tag ')+' '+(item.LstWIN==1?'bg-orange':'bg-cyan'))}}">{{picker[item.LstType]}}</view><view class="{{((' cu-tag ')+' '+(item.LstWIN==1?'bg-orange':'bg-cyan'))}}">{{item.LstMoney>0?'赢':'输'}}</view><view class="{{(('cu-tag ')+' '+(item.LstWIN==1?'line-orange':'line-cyan'))}}">{{item.LstMoney+"元"}}</view><view class="{{(('cu-tag ')+' '+(item.LstWIN==1?'bg-orange':'bg-cyan'))}}">地点</view><view class="{{(('cu-tag ')+' '+(item.LstWIN==1?'line-orange':'line-cyan'))}}">{{item.LstAddress}}</view><view class=" bg-grey padding-xs radius margin-top-xs  text-sm"><view class="flex"><view>牌友 :</view><view class="flex-sub">{{item.LstFriends}}</view></view></view><view class="margin-top-xs flex justify-between"><view class="text-gray text-df">{{item.LstDate}}</view><view><button class="cu-btn  sm bg-orange margin-right-xl" data-index="{{index}}" data-event-opts="{{[['tap',[['on_edit',['$event']]]]]}}" onTap="__e">编辑</button><button class="cu-btn  sm bg-grey" data-index="{{index}}" data-event-opts="{{[['tap',[['showModal',['$event']]]]]}}" onTap="__e">删除</button></view></view></view></view></view></block><view class="{{(('cu-modal')+' '+(modalName=='DelDialog'?'show':''))}}"><view class="cu-dialog"><view class="cu-bar bg-white justify-end"><view class="content">删除输赢记录</view><view data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="action" onTap="__e"><text class="cuIcon-close text-red"></text></view></view><view class="padding-xl">{{"你确定删除该条信息吗？"+lstIndex}}</view><view><view class="padding-xs text-sm"><view class="{{(('cu-tag ')+' '+(WinList[lstIndex].LstWIN==1?'bg-orange':'bg-cyan'))}}">日期</view><view class="{{(('cu-tag ')+' '+(WinList[lstIndex].LstWIN==1?'line-orange':'line-cyan'))}}">{{WinList[lstIndex].LstDate}}</view></view><view class="padding-xs text-sm"><view class="{{(('cu-tag ')+' '+(WinList[lstIndex].LstWIN==1?'bg-orange':'bg-cyan'))}}">{{picker[WinList[lstIndex].LstType]}}</view><view class="{{((' cu-tag ')+' '+(WinList[lstIndex].LstWIN==1?'bg-orange':'bg-cyan'))}}">{{WinList[lstIndex].LstMoney>0?'赢':'输'}}</view><view class="{{(('cu-tag ')+' '+(WinList[lstIndex].LstWIN==1?'line-orange':'line-cyan'))}}">{{WinList[lstIndex].LstMoney+"元"}}</view><view class="{{(('cu-tag ')+' '+(WinList[lstIndex].LstWIN==1?'bg-orange':'bg-cyan'))}}">地点</view><view class="{{(('cu-tag ')+' '+(WinList[lstIndex].LstWIN==1?'line-orange':'line-cyan'))}}">{{WinList[lstIndex].LstAddress}}</view></view><view class="margin-top-xs flex justify-between"><view class="text-gray text-df"></view></view></view><view class="cu-bar bg-white justify-end"><view class="action"><button data-event-opts="{{[['tap',[['hideModal',['$event']]]]]}}" class="cu-btn line-green text-green" onTap="__e">取消</button><button data-event-opts="{{[['tap',[['on_del',['$event']]]]]}}" class="cu-btn bg-green margin-left" onTap="__e">确定</button></view></view></view></view></view>